; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Magenta Arcade II"
config/version="1.0.7"
config/tags=PackedStringArray("longhat")
run/main_scene="res://ma2_main_scene.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.4", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://elements/ui/logos/app_splash.png"
config/icon="res://elements/ui/logos/app_icon.png"

[aseprite]

animation/layers/exclusion_pattern="#.*"
import/import_plugin/default_automatic_importer="SpriteFrames"

[audio]

general/text_to_speech=true

[autoload]

WwiseRuntimeManager="*res://addons/Wwise/runtime/wwise_runtime_manager.gd"
SaveManager="*res://modules/game_save_system/game_save_manager.gd"
LevelManager="*res://systems/level/level_manager.tscn"
Menu="*res://elements/ui/menus/pause_menu/pause_menu.tscn"
AudioManager="*res://systems/audio_wwise/audio_manager.gd"
TextManager="*res://systems/ui/text_manager.gd"
DevManager="*res://modules/dev_manager/scenes/dev_manager.tscn"
Ma2MetaManager="*res://systems/ma2_meta/ma2_meta_manager.gd"
GameInit="*res://game_init.tscn"
TimeManager="*res://systems/time/TimeManager.tscn"
AccessibilityManager="*res://modules/accessibility_manager/AccessibilityManager.tscn"
FakeCursor="*res://marketing_tools/fake_hand/fake_cursor.tscn"
SocialPlatformManager="*res://modules/social_platform_system/social_platform_manager.gd"
GodotPlayGameServices="*res://addons/GodotPlayGameServices/scripts/autoloads/godot_play_game_services.gd"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=900
window/size/viewport_height=1800
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1
window/vsync/vsync_mode=2

[dotnet]

project/assembly_name="Ma2_Proto"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/GodotPlayGameServices/plugin.cfg", "res://addons/gap/plugin.cfg", "res://addons/google_sheet_importer/plugin.cfg", "res://addons/safe_area_rect/plugin.cfg")

