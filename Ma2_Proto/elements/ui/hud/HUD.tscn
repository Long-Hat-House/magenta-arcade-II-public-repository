[sub_resource type="GDScript" id="GDScript_p2yg3"]
script/source = "extends Control
@export var _anim:AnimationPlayer
@export var _heal_bar:TextureProgressBar

func _ready() -> void:
	visible = Player.instance.currentState.emergency_heal_level > 0

	if !visible:
		return

	await get_tree().process_frame
	Player.instance.hp_change.connect(_on_hp_change)
	Player.instance.emergency_heal.healing_start.connect(_on_healing_start)
	Player.instance.emergency_heal.healing_decayed.connect(_on_healing_start)
	Player.instance.emergency_heal.healing_completed.connect(_on_healing_start)
	Player.instance.emergency_heal.healing_change_total.connect(_on_healing_change_total)
	_heal_bar.value = 0
	_heal_bar.visible = false

func _update_heal_animation():
	if Player.instance.emergency_heal.is_healing():
		_anim.play(\"healing\")
		_heal_bar.visible = true
	elif Player.instance.emergency_heal.can_heal():
		_anim.play(\"warning\")
		_heal_bar.visible = false
	else:
		_anim.play(\"off\")
		_heal_bar.visible = false

func _on_hp_change(hp:int, position:Vector3):
	_update_heal_animation()

func _on_healing_start():
	_update_heal_animation()

func _on_healing_decayed():
	_update_heal_animation()

func _on_healing_completed():
	_update_heal_animation()

func _on_healing_change_total(val:float):
	_heal_bar.value = val
"

