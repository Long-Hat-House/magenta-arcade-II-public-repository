[sub_resource type="GDScript" id="GDScript_dj228"]
script/source = "extends ProgressBarIndexedElementSwitchAnimation

@export var _to_set_size:Array[Control]
@export var _width:int = 64
@export var _height_min:int = 64
@export var _height_max:int = 128

@export var _warning_animation:Switch_Oning_Offing_AnimationPlayer

func set_state(index:int, max:int, fill:int, imediate:bool):
	super.set_state(index, max, fill, imediate)

	max -= 1
	if max <= 0: return

	var height = lerp(_height_min, _height_max, float(index)/max)
	if height > _height_max:
		height = _height_max

	for obj in _to_set_size:
		obj.custom_minimum_size = Vector2(_width, height)

	_warning_animation.set_switch(fill <= 1)
"

