[sub_resource type="GDScript" id="GDScript_4raau"]
script/source = "extends Button

var _time_down:float

func _ready() -> void:
	button_down.connect(on_button_down)
	button_up.connect(on_button_up)

func on_button_down():
	_time_down = Time.get_ticks_msec()

func on_button_up():
	if _time_down < 0 : return
	var dif:float = Time.get_ticks_msec() - _time_down
	_time_down = -1
	if dif > 3000: #toggle
		DevManager.toggle_developer_tools()
"

