[sub_resource type="GDScript" id="GDScript_onper"]
resource_name = "Version Number"
script/source = "extends Label


# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	while DevManager._initialized != true:
		await get_tree().process_frame
		if !is_instance_valid(self): return
	text = \"\"

	if DevManager.test_info:
		text = DevManager.test_info.test_name + \" \"

	text += \"v\"+ProjectSettings.get_setting(\"application/config/version\")
	pass # Replace with function body.


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	pass
"

