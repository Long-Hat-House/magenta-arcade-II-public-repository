[sub_resource type="GDScript" id="GDScript_gemo0"]
resource_name = "GoToEnd"
script/source = "extends Control

@export var animator:Switch_Oning_Offing_AnimationPlayer
@export var player:TextFlowPlayerZap
@export var button:Button

func _ready() -> void:
	button.pressed.connect(player.go_to_end)
	player.new_zap_finished.connect(_on_new_zap)

func _recheck() -> void:
	if animator.is_set_to_on() && player.is_at_end():
		animator.set_switch(false)
	elif animator.is_set_to_off() && !player.is_at_end():
		animator.set_switch(true)

func _on_new_zap(speaker:TextFlowZapSpeaker, message:String):
	_recheck()
"

